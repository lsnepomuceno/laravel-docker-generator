{
    "name": "lsnepomuceno/laravel-docker-generator",
    "description": "A description for laravel-docker-generator.",
    "type": "package",
    "license": "MIT",
    "homepage": "https://github.com/lsnepomuceno/laravel-docker-generator",
    "support": {
        "issues": "https://github.com/lsnepomuceno/laravel-docker-generator/issues",
        "source": "https://github.com/lsnepomuceno/laravel-docker-generator"
    },
    "keywords": [
        "laravel",
        "docker",
        "docker generator",
        "lsnepomuceno"
    ],
    "authors": [
        {
            "name": "lsnepomuceno",
            "email": "lsn.nepomuceno@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0",
        "symfony/yaml": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Lsnepomuceno\\LaravelDockerGenerator\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lsnepomuceno\\LaravelDockerGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lsnepomuceno\\LaravelDockerGenerator\\LaravelDockerGeneratorServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
